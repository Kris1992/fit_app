{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "2.1.5",
        "gedmo/doctrine-extensions": "^2.4",
        "google/recaptcha": "^1.2",
        "knplabs/knp-paginator-bundle": "^4.0",
        "oneup/flysystem-bundle": "^3.1",
        "sensio/framework-extra-bundle": "^5.4",
        "sensiolabs/security-checker": "^6.0",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dependency-injection": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/messenger": "4.3.*",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "4.3.*",
        "symfony/serializer": "4.3.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/web-server-bundle": "4.3.*",
        "symfony/yaml": "4.3.*",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "behat/behat": "dev-master",
        "behat/mink": "dev-master",
        "behat/mink-extension": "dev-master",
        "symfony/profiler-pack": "^1.0",
        "symfony/debug-pack": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "3.0.2",
        "friends-of-behat/symfony-extension": "2.0.x-dev",
        "fzaninotto/faker": "^1.8",
        "symfony/maker-bundle": "^1.12",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ewbarnard/Mink.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ewbarnard/MinkExtension.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jfsenechal/SymfonyExtension.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jfsenechal/contexts.git"
        }
    ]

}
